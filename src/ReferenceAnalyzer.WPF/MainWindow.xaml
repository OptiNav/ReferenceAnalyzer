<reactiveui:ReactiveWindow 
        x:Class="ReferenceAnalyzer.WPF.MainWindow"
        x:TypeArguments="local:AppViewModel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReferenceAnalyzer.WPF"
        xmlns:reactiveui="http://reactiveui.net"
        mc:Ignorable="d"
        Title="ReferenceExplorer" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <TextBox Name="Path" DockPanel.Dock="Top"></TextBox>
        </DockPanel>
    </Grid>
</reactiveui:ReactiveWindow>
