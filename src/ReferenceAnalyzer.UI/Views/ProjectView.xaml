<rxui:ReactiveUserControl
    x:Class="ReferenceAnalyzer.UI.Views.ProjectView"
    x:TypeArguments="models:Project"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="clr-namespace:ReferenceAnalyzer.UI.ViewModels;assembly=ReferenceAnalyzer.UI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:ReferenceAnalyzer.Core;assembly=ReferenceAnalyzer.Core"
    xmlns:system="clr-namespace:System;assembly=System.Runtime"
    xmlns:rxui="http://reactiveui.net"
    xmlns:models="clr-namespace:ReferenceAnalyzer.Core.Models;assembly=ReferenceAnalyzer.Core"
    mc:Ignorable="d" >

    <StackPanel Orientation="Horizontal">
        <TextBlock Name="Progress" Width="10" Height="10" />
        <TextBlock Name="DisplayName" />
    </StackPanel>
</rxui:ReactiveUserControl>
